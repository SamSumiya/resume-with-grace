{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    // "noImplicitAny": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react"
  },
  "exclude": ["node_modules"],
  "include": ["../vite.config.ts", "**/*.ts", "**/*.tsx"]
}

